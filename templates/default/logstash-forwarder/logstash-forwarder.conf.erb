{
  "network": {
    "servers": [ "localhost:5043" ],
    "ssl key": "<%= @options['key_location'] %>",
    "ssl ca": "<%= @options['crt_location'] %>",
    "timeout": 15
  },
  "files": [
    {
      "paths": [
        "/var/log/messages",
        "/var/log/*.log"
      ],

      "fields": { "type": "syslog" }
    }
  ]
}
